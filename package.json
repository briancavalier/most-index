{
  "name": "@most/index",
  "version": "1.0.0",
  "description": "Count and index @most/core events",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "microbundle -i src/index.ts -f es,cjs",
