  "keywords": ["most", "mostjs", "reactive", "streams"],
  "author": "brian@briancavalier.com",
  "license": "MIT",
  "dependencies": {
    "@most/core": "^1.3.4"
  },
  "devDependencies": {
    "@most/scheduler": "^1.2.2",
    "@most/types": "^1.0.1",
    "@typed/test": "^9.1.0",
    "microbundle": "^0.11.0",
    "prettier": "^1.16.4",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.4.1"
  }
